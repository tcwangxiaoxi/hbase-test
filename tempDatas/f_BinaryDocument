<script type="text/javascript">
  var head_photo = "" ? "" : ''; //评论头像
  var qid = 367428 ? 367428 : 0; //问题id
   
  //添加评论回答                 
  function addAnswer(id,obj,uid) {
    //判断是否登录
    if (!to8to_uid) {
      document.domain = 'shejiben.com';
      showSingleLogin(2,2);
      return false;
    }
    //评论框
    var _html = jq('<div class="div_pl" id="box_'+id+'"><div class="pl_write"><div class="d_img"><img src="'+head_photo+'"></div><div class="d_content d_content01"><form method="post" enctype="multipart/form-data" name="detailForm"><em></em><label class="label_txt">请输入评论内容...</label><textarea class="da_input input_name edit_answer" type="text" name="ask_answer"  autocomplete="off"></textarea><input type="hidden" value="2" name="type" /><input type="hidden" value="'+id+'" name="toid"  /><input type="hidden" value="'+qid+'" name="id" /><input class="d_smbmit  dis_submit" type="submit" value="发布评论"></form></div><div  class="clear"></div></div></div>');
    jq(obj).parent().parent().append(_html);

    jq(obj).attr('onclick',"closeAnswerBox("+id+",this)");
  }
  //点击关闭评论回答
  function closeAnswerBox(id,obj) {
    jq('#box_'+id).remove();
    jq(obj).attr('onclick',"addAnswer("+id+",this)");
  }
  //点赞
  function praiseAns (anid,obj){
    var url = 'http://ask.shejiben.com/k.php?act=ask';
    var data = {anid:anid,type:3};
      jq.post(url,data,function(msg){       
        if(msg){   
          jq("#praise_"+anid).html(msg);
        }else {
          alert("您已经赞过该回答了！");
        }      
    });
  }

//点击图片上传
jQuery('#del_up_file').hide();
jQuery('#file').bind('change',function() {
    var filename = jq(this).val();
    var fn_arr = new Array();
        fn_arr = filename.split('\\');
        var new_fn = '';
        for(var i = 0;i<fn_arr.length;i++){
          new_fn = fn_arr[i] ;
        }
        if(new_fn.length > 10){
          var file_index = new_fn.lastIndexOf('.');
          var file_end = new_fn.length;
          var file_suffix = new_fn.substring(file_index,file_end);
          file_pr = new_fn.substr(0,5);
          new_fn = file_pr+'...'+file_suffix;
        }
    jq("#file_name").html(new_fn);
    jQuery('#del_up_file').show();
    var fileType = filename.substr(filename.indexOf(".")+1).toUpperCase(); 
    if(fileType!="BMP"&&fileType!="PNG"&&fileType!="GIF"&&fileType!="JPG"&&fileType!="JPEG"){
       jQuery('#file_name').html('');
       jQuery('#del_up_file').hide();
       alert("图片限于bmp,png,gif,jpeg,jpg格式");
     }
});

//点击图片删除
jQuery("#del_up_file").live("click", function(){ 
    jQuery("#file").val('');
    jQuery("#file_name").html('');
    jQuery('#del_up_file').hide();
});

//异步获取回答
 var page = 2;
 var id = 367428;
 var pagenum = 7;
  jQuery(".answer_more").click(function(){    
    jQuery.ajax({
        url: 'http://ask.shejiben.com/d.php?uid='+id,
        data: {'page': page,'type': 'ask'},
        dataType: 'json',
        success: function(result) {
            if (result) {
                var html = '';
                for (var i = 0; i < result.length; i++) {
                  html += '<li>';
                  html += '<div class="d_img"><img width="50" height="50" src="'+result[i]['head_photo']+'"></div>';
                  html += '<div class="d_content">';
                  html += '<p class="p_01">';
                  html += '<a href="'+result[i]['url']+'" target="_blank" class="a_name">'+result[i]['nick']+'</a>';
                  html += '<i>'+result[i]['createtime']+'</i>';
                  html += '<span  class="clear"></span>';
                  html += '</p>';
                  html += '<p class="p_02">'+result[i]['content']+'</p>';

                  if(result[i]['pic']){
                   html += '<img alt="'+result[i]['pic']+'" width="80" height="80" src="'+result[i]['content']+'">';
                  }

                  html += '<p class="p_03">';
                  if( result[i]['client'] ){
                    html += '<a href="http://www.shejiben.com/app/" rel="nofollow" target="_blank" class="a_sjb a_sjb00">来自设计本'+result[i]['client']+'客户端</a>';
                  }

                  html +='<a href="javascript:void(0)" onclick="praiseAns('+result[i]['id']+',this)"  class="a_dz addzan"><em ></em><i id="praise_'+result[i]['id']+'">'+result[i]['praise_num']+'</i></a>';
                  html += ' <a href="javascript:void(0)" onclick="addAnswer('+result[i]['id']+',this,'+result[i]['answer_uid']+')" class="a_pl"><em></em>评论</a><span  class="clear"></span>';
                  html += '</p></div><div  class="clear"></div></li>';
                }       
             }
                jQuery(".detail_left_mid ul li").last().after(html);
                if (result !== null && page<pagenum ) {
                    jq(".answer_more").html('载入更多');
                    page = page + 1;
                } else{
                    jq(".answer_more").remove();
                } 
           }
        });
    });
</script></div>
